{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "‚ùØ</>_: ",
    "foreground": "#39b01f",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "<#ffffff>\ue266 </>{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB",
          "foreground": "#be9ddf",
          "background": "#272727",
          "type": "sysinfo",
          "style": "plain"
        },
        {
          "properties": {
            "style": "austin",
            "threshold": 0
          },
          "template": " \u27e8{{ .FormattedMs }}\u27e9",
          "foreground": "#ffe66d",
          "background": "#272727",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "template": " {{ if eq .Type \"wifi\"}}\uf1eb{{else if eq .Type \"ethernet\"}}\ueba9{{ end }} ",
          "foreground": "#ffffff",
          "background": "#272727",
          "type": "connection",
          "style": "diamond"
        },
        {
          "foreground": "#111111",
          "background": "transparent",
          "type": "upgrade",
          "style": "plain"
        },
        {
          "template": "\ue0b0",
          "foreground": "#272727",
          "background": "transparent",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": "<#000000>‚ùØ</>",
            "home_icon": "~",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "trailing_diamond": "\ue0b0",
          "template": "\ue5ff {{ .Path }}",
          "foreground": "#07bca4",
          "background": "#272727",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "fetch_upstream_icon": true,
            "untracked_modes": {}
          },
          "trailing_diamond": "\ue0b0",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#FFEB3B>\uf044 {{ .Working.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}",
          "foreground": "#f05030",
          "background": "#272727",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ]
        },
        {
          "trailing_diamond": "\ue0b0",
          "template": "ü¶Ä {{ .Full }}",
          "foreground": "#e8276f",
          "background": "#272727",
          "type": "rust",
          "style": "diamond"
        },
        {
          "properties": {
            "display_mode": "context"
          },
          "trailing_diamond": "\ue0b0",
          "template": "<#31709f>\ue235 </>{{ .Full }}",
          "foreground": "#ffdc4e",
          "background": "#272727",
          "type": "python",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "template": "‚ùØ</>_: ",
          "foreground": "#39b01f",
          "type": "status",
          "style": "diamond"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
